<?xml version="1.0" encoding="utf-8" ?>
<fsm xmlns="urn:schema-lwj:fsm"
     xmlns:x="urn:schema-lwj:expression"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:schema-lwj:fsm ../../../resources/lwj.fsm.xsd
      urn:schema-lwj:expression ../../../resources/lwj.expr.xsd"
     initial="s1"
   >
  <parallel name="s1">
    <transition target="s2" event="to.s2" />
    <onEntry>
      <log msg="s1.entry"/>
    </onEntry>
    <onExit>
      <log msg="s1.exit"/>
    </onExit>
    <state name="s11">
      <onEntry>
        <log msg="s11.entry"/>
      </onEntry>
      <onExit>
        <log msg="s11.exit"/>
      </onExit>
      <log msg="s11.update" />
    </state>
    <state name="s12">
      <onEntry>
        <log msg="s12.entry"/>
      </onEntry>
      <onExit>
        <log msg="s12.exit"/>
      </onExit>
      <log msg="s12.update"/>
    </state>
  </parallel>
  <state name="s2">
    <onEntry>
      <log msg="s2.entry"/>
    </onEntry>
    <onExit>
      <log msg="s2.exit"/>
    </onExit>
  </state>
</fsm>